{
    "name": "token27/cakephp-scheduled-plugin",
    "description": "Scheduled plugin for CakePHP.",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "schedule", "background", "cronjobs"],
    "license": "MIT",	
    "authors": [
        {
            "name": "Token27",
            "homepage": "https://github.com/token27/",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0",
        "lavary/crunz": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Scheduled\\": "src/",
            "Scheduled\\Test\\Fixture\\": "tests/Fixture/"
        }
    },
    "autoload-dev": {
        "psr-4": {            
            "Scheduled\\Test\\": "tests/"
        }
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "process-timeout": 600
    }
}
